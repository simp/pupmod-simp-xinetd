service <%= @name %>
{
    socket_type = <%= @socket_type %>
    user = <%= @user %>
    server = <%= @server %>
    wait = <%= @x_wait %>
    disable = <%= @disable %>
    log_type = <%= @log_type %>
    log_on_success = <%= @log_on_success %>
    log_on_failure = <%= @log_on_failure %>
    port = <%= @port %>
    umask = <%= @x_umask %>
<% if ! @x_id.eql?("nil") then -%>
    id = <%= @x_id %>
<% end -%>
<% if ! @x_type.eql?("nil") then -%>
    type = <%= @x_type %>
<% end -%>
<% if ! @flags.eql?("nil") then -%>
    flags = <%= @flags %>
<% end -%>
<% if ! @protocol.eql?("nil") then -%>
    protocol = <%= @protocol %>
<% end -%>
<% if ! @group.eql?("nil") then -%>
    group = <%= @group %>
<% end -%>
<% if ! @nice.eql?("nil") then -%>
    nice = <%= @nice %>
<% end -%>
<% if ! @server_args.eql?("nil") then -%>
    server_args = <%= @server_args %>
<% end -%>
<% if ! @libwrap.eql?("nil") then -%>
    libwrap = <%= @libwrap %>
<% end -%>
<% if !@trusted_nets.empty? then -%>
    only_from = <%= scope.function_nets2cidr(@trusted_nets).join(' ') %>
<% end -%>
<% if ! @instances.eql?("nil") then -%>
    instances = <%= @instances %>
<% end -%>
<% if ! @access_times.eql?("nil") then -%>
    access_times = <%= @access_times %>
<% end -%>
<% if ! @rpc_version.eql?("nil") then -%>
    rpc_version = <%= @rpc_version %>
<% end -%>
<% if ! @rpc_number.eql?("nil") then -%>
    rpc_number = <%= @rpc_number %>
<% end -%>
<% if ! @env.eql?("nil") then -%>
    env = <%= @env %>
<% end -%>
<% if ! @passenv.eql?("nil") then -%>
    passenv = <%= @passenv %>
<% end -%>
<% if ! @redirect_ip.eql?("nil") then -%>
    redirect = <%= @redirect_ip -%>
<% if ! @redirect_port.eql?("nil") then -%>
 <%= @redirect_port -%>
<% end -%>
<% end %>
<% if ! @x_bind.eql?("nil") then -%>
    bind = <%= @x_bind %>
<% end -%>
<% if ! @banner.eql?("nil") then -%>
    banner = <%= @banner %>
<% end -%>
<% if ! @banner_success.eql?("nil") then -%>
    banner_success = <%= @banner_success %>
<% end -%>
<% if ! @banner_fail.eql?("nil") then -%>
    banner_fail = <%= @banner_fail %>
<% end -%>
<% if ! @per_source.eql?("nil") then -%>
    per_source = <%= @per_source %>
<% end -%>
<% if ! @cps.eql?("nil") then -%>
    cps = <%= @cps %>
<% end -%>
<% if ! @max_load.eql?("nil") then -%>
    max_load = <%= @max_load %>
<% end -%>
<% if ! @groups.eql?("nil") then -%>
    groups = <%= @groups %>
<% end -%>
<% if ! @mdns.eql?("nil") then -%>
    mdns = <%= @mdns %>
<% end -%>
<% if ! @enabled.eql?("nil") then -%>
    enabled = <%= @enabled %>
<% end -%>
<% if ! @include.eql?("nil") then -%>
    include = <%= @include %>
<% end -%>
<% if ! @includedir.eql?("nil") then -%>
    includedir = <%= @includedir %>
<% end -%>
<% if ! @rlimit_as.eql?("nil") then -%>
    rlimit_as = <%= @rlimit_as %>
<% end -%>
<% if ! @rlimit_cpu.eql?("nil") then -%>
    rlimit_cpu = <%= @rlimit_cpu %>
<% end -%>
<% if ! @rlimit_data.eql?("nil") then -%>
    rlimit_data = <%= @rlimit_data %>
<% end -%>
<% if ! @rlimit_rss.eql?("nil") then -%>
    rlimit_rss = <%= @rlimit_rss %>
<% end -%>
<% if ! @rlimit_stack.eql?("nil") then -%>
    rlimit_stack = <%= @rlimit_stack %>
<% end -%>
<% if ! @deny_time.eql?("nil") then -%>
    deny_time = <%= @deny_time %>
<% end -%>
}
